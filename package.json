{
  "name": "ngx-configure-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build --prod ngx-configure",
    "npm_pack": "cd dist/ngx-configure && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "copy": "cp README.md dist/ngx-configure/README.md",
    "prepare": "npm run build_lib && npm run copy",
    "publish": "npm run prepare && cd dist/ngx-configure && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^8.1.0",
    "@angular/core": "^8.1.0",
    "@angular/compiler": "^8.1.0",
    "@angular/animations": "^8.1.0",
    "@angular/forms": "^8.1.0",
    "@angular/platform-browser": "^8.1.0",
    "@angular/platform-browser-dynamic": "^8.1.0",
    "@angular/router": "^8.1.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^8.1.0",
    "@angular-devkit/build-ng-packagr": "~0.801.0",
    "@angular-devkit/build-angular": "~0.801.0",
    "ng-packagr": "^5.1.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.0",
    "typescript": "3.4.5",
    "@angular/cli": "~8.1.0",
    "@angular/language-service": "^8.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.18.0"
  }
}
